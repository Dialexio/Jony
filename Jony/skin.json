{
	"name": "Jony",
	"author": [
		"Dialexio",
		"iAdam1n",
		"..."
	],
	"version": "1.0.7",
	"url": "https://github.com/Dialexio/Jony",
	"descriptionmsg": "jony-skin-desc",
	"namemsg": "skinname-jony",
	"license-name": "GPL-2.0+",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.26.0"
	},
	"ValidSkinNames": {
		"jony": "Jony"
	},
	"MessagesDirs": {
		"Jony": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.jony.styles": {
			"class": "ResourceLoaderSkinModule",
			"features": {
				"content": true,
				"content-thumbnails": true,
				"content-tables": true,
				"elements": true,
				"i18n-ordered-lists": true,
				"i18n-all-lists-margins": true,
				"i18n-headings": true,
				"interface": true,
				"interface-message-box": true,
				"interface-category": true,
				"logo": true,
				"content-links": true,
				"content-links-external": true
			},
			"styles": {
				"resources/print.less": {
					"media": "print"
				},
				"resources/screen.less": {
					"media": "screen"
				}
			}
		},
		"skins.jony": {
			"position" : "top",
			"styles": {
				"resources/print.less": {
					"media": "print"
				},
				"resources/screen.less": {
					"media": "screen"
				}
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Jony"
	},
	"AutoloadClasses": {
		"SkinJony": "Jony.skin.php",
		"JonyTemplate": "JonyTemplate.php"
	},
	"manifest_version": 1
}
